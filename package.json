  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "author": "Ale M <ale@sketch.com>",
  "repository": "https://github.com/sketch-hq/color-variables-migrator.git",
  "dependencies": {
    "sketch-module-web-view": "^3.4.3"
  }
