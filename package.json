{
  "name": "color-variables-migrator",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "color-variables-migrator",
    "manifest": "src/manifest.json",
    "main": "color-variables-migrator.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
